{
  "name": "eLecture",
  "version": "0.0.1",
  "description": "Online platform to facilitate communication between instructors and students during lecture",
  "repository": "https://github.com/TsaiAnson/eLecture.git",
  "main": "server/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack -p --config ./build/webpack.config.js",
    "build:dev": "cross-env NODE_ENV=development webpack -d --config ./build/webpack.config.js",
    "dev": "npm run build:dev & npm run start:dev",
    "prod": "npm run build & npm start",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "start:dev": "cross-env NODE_ENV=development node server/index.js",
    "test": "cross-env NODE_ENV=test mocha --recursive"
  },
  "author": "Anson, Jove, Titan",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.2",
    "babel-core": "^6.25.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.17.2",
    "connect-mongo": "^1.3.2",
    "express": "^4.15.3",
    "express-session": "^1.15.3",
    "mongoose": "^4.11.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "socket.io": "^2.0.3"
  },
  "devDependencies": {
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.0",
    "chai-http": "^3.0.0",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "mocha": "^3.4.2",
    "style-loader": "^0.18.2",
    "webpack": "^3.2.0"
  }
}
